	ASSERTU4(f4_to_u4_convert(-U1_55), U41_AB);
	ASSERTU4(f4_to_u4_convert(-U1_AA), U41_56);
	ASSERTU4(f4_to_u4_convert(-U2_55), U42_AB);
	ASSERTU4(f4_to_u4_convert(-U2_AA), U42_56);
	ASSERTU4(f4_to_u4_convert(-U4_55), 0xaaaaaa80);
