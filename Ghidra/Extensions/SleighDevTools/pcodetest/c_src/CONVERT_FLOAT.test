	ASSERTU4(f4_to_u4_convert((f4)PI_SHORT), 3);
	ASSERTU4(f4_to_u4_convert((f4)-PI_SHORT), 0xfffffffd);
	ASSERTU4(f4_to_u4_convert((f4)M_PI), 3);
	ASSERTU4(f4_to_u4_convert((f4)-M_PI), 0xfffffffd);
