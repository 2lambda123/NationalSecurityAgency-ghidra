	ASSERTI2(pcode_bswap_i2(-1), -1);
	ASSERTI2(pcode_bswap_i2(I2_MIN), 128);
	ASSERTI2(pcode_bswap_i2(I2_MAX), -129);
	ASSERTI2(pcode_bswap_i2(0x1234), 0x3412);
	ASSERTI2(pcode_bswap_i2(0x0080), I2_MIN);
}


